{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenIdConnectOptions": {
    "CookieName": "__BFF",
    "ClientId": "product-feedback",
    "ClientSecret": "",
    "ResponseType": "code",
    "ResponseMode": "query",
    "GetClaimsFromUserInfoEndpoint": true,
    "MapInboundClaims": false,
    "SaveTokens": true,
    "Scope": "openid profile offline_access",
    "AuthorityPath": "realms/development",
    "RedirectUri": "/"
  },
  "ReverseProxy": {
    "Routes": {
      "product-feedback": {
        "ClusterId": "product-feedback-api",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "product-feedback-api": {
        "Destinations": {
          "destination1": {
            "Address": "http://minimalapiapp:5185"
          }
        }
      }
    }
  }
}
